# Vector Database (Upstash)
UPSTASH_VECTOR_REST_URL="your-upstash-vector-url"
UPSTASH_VECTOR_REST_TOKEN="your-upstash-vector-token"

# PostgreSQL Database (Neon)
NEON_DATABASE_URL="your-neon-database-url"
NEON_DATABASE_URL_DIRECT="your-neon-direct-database-url"

# LLM Provider (Groq)
GROQ_API_KEY="your-groq-api-key"

# OpenAI (for Whisper Speech-to-Text)
OPENAI_API_KEY="your-openai-api-key"

# App Configuration
NODE_ENV="development"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Security (generate these for production)
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
ENCRYPTION_KEY="your-32-char-encryption-key-here"

# NextAuth Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-key-change-in-production"

# Google OAuth (Get these from Google Cloud Console)
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# Basic Auth (for development/staging)
BASIC_AUTH_USERNAME="your-username"
BASIC_AUTH_PASSWORD="your-secure-password"

# Twilio Voice Integration
TWILIO_ACCOUNT_SID="your-twilio-account-sid"
TWILIO_AUTH_TOKEN="your-twilio-auth-token"
TWILIO_PHONE_NUMBER="your-twilio-phone-number"

# Option 7 Feature Flags & Rollback
FEATURE_CAREER_TRACKS="true"
ROLLBACK_TO_ORIGINAL="false"
CAREER_TRACK_ROLLOUT="100"